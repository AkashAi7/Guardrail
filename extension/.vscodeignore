# VS Code Marketplace - Files to exclude from published extension

# Source files (we include compiled JS only)
src/**
tsconfig.json
*.ts

# Build artifacts
*.map

# Development files
.vscode/**
.vscode-test/**

# Exclude MOST of node_modules but keep production dependencies
node_modules/**
!node_modules/axios/**
!node_modules/proxy-from-env/**
!node_modules/follow-redirects/**
!node_modules/form-data/**
!node_modules/combined-stream/**
!node_modules/delayed-stream/**
!node_modules/mime-types/**
!node_modules/mime-db/**
!node_modules/asynckit/**

# Git
.git/**
.gitignore

# NPM
package-lock.json
npm-debug.log
yarn-error.log

# Testing
test/**
*.test.js
*.test.ts

# Documentation (keep README.md and CHANGELOG.md)
# docs/**

# Misc
.DS_Store
Thumbs.db
*.vsix
